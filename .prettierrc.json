{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "bracketSpacing": false,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": false,
  "ignorePath": ".prettierignore"
}
